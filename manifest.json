    "launch_path": "setting.html",/*应用的入口页面，默认为根目录下的index.html；支持网络地址，必须以http://或https://开头*/
    "developer": {
        "name": "",/*开发者名称*/
        "email": "583181285@qq.com",/*开发者邮箱地址*/
        "url": "http://www.dcloud.io"/*开发者个人主页地址*/
